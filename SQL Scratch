CREATE TABLE properties (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  addressline1 TEXT NOT NULL,
  sqft INTEGER
);

DROP TABLE properties

CREATE TABLE properties (
  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
  street TEXT,
  city TEXT,
  state TEXT,
  zip_code TEXT,
  sqft INTEGER,
  year_built INTEGER
);
